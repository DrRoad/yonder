{
  "name": "dull",
  "version": "0.0.5",
  "description": "Reimagine shiny applications.",
  "main": "lib/js/dull.min.js",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "feather-icons": "^4.7.0",
    "file-saver": "^1.3.8",
    "flatpickr": "^4.4.6",
    "ion-rangeslider": "^2.2.0",
    "jquery": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.100.2",
    "popper.js": "^1.14.3",
    "velocity-animate": "^1.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "clean-css": "^4.1.11",
    "clean-css-cli": "^4.1.11",
    "commander": "^2.15.1",
    "concat": "^1.0.3",
    "eslint": "^4.19.1",
    "glob": "^7.1.2",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^2.5.0"
  },
  "scripts": {
    "test": "echo \"Sorry, nothing here\"",
    "js-lint": "eslint src/js/",
    "js-update": "npm run js-compile && cp lib/js/dull.min.js ../inst/www/dull/js/dull.min.js",
    "js-compile": "cat src/js/**.js | babel --compact --minified --no-comments > lib/js/dull.min.js",
    "css-lint": "stylelint --syntax scss src/scss/**/*.scss",
    "css-compile": "node-sass --output-style expanded src/scss/dull.scss lib/css/dull.css",
    "css-prefix": "postcss --config build/postcss.config.js --replace lib/css/dull.css",
    "css-minify": "cleancss --level 1 --output lib/css/dull.min.css lib/css/dull.css",
    "css-vend": "cp lib/css/dull.min.css ../inst/www/dull/css/",
    "css": "npm-run-all css-lint css-compile css-prefix css-prefix css-minify css-vend",
    "font-awesome-update": "npm run font-awesome-checkout &> /dev/null && npm run --silent font-awesome-js > ../inst/www/font-awesome/js/fontawesome.js && npm run --silent font-awesome-css > ../inst/www/font-awesome/css/fontawesome.min.css",
    "font-awesome-js": "cat vendors/Font-Awesome/svg-with-js/js/fontawesome-all.min.js",
    "font-awesome-css": "cat vendors/Font-Awesome/svg-with-js/css/fa-svg-with-js.css",
    "font-awesome-checkout": "npm run font-awesome-clone && cd vendors/Font-Awesome && git checkout tags/$(git describe --tags --abbrev=0)",
    "font-awesome-clone": "cd vendors && test -d Font-Awesome || git clone https://github.com/FortAwesome/Font-Awesome.git",
    "material-design-icons-update": "npm update material-design-icons && npm run material-design-css",
    "material-design-icons-css": "cp node_modules/material-design-icons/iconfont/*.{woff,css} ../inst/www/material-design/",
    "feather-update": "npm update feather && npm run feather-js && npm run feather-svg",
    "feather-js": "cp node_modules/feather-icons/dist/feather.min.js ../inst/www/feather/",
    "feather-svg": "cp node_modules/feather-icons/dist/feather-sprite.svg ../inst/www/feather/",
    "bootstrap-js-update": "npm update bootstrap && cp node_modules/bootstrap/dist/js/bootstrap.min.js ../inst/www/bootstrap/js/",
    "bootstrap-css-compile": "node-sass --output-style expanded src/scss/vendors/bootstrap.scss lib/css/bootstrap.css",
    "bootstrap-css-prefix": "postcss --config build/postcss.config.js --replace lib/css/bootstrap.css",
    "bootstrap-css-minify": "cleancss --level 1 --output lib/css/bootstrap.min.css lib/css/bootstrap.css",
    "bootstrap-css-vend": "cp lib/css/bootstrap.min.css ../inst/www/bootstrap/css/",
    "bootstrap-css": "npm-run-all bootstrap-css-compile bootstrap-css-prefix bootstrap-css-minify bootstrap-css-vend"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nteetor/dull.git"
  },
  "keywords": [
    "shiny",
    "reactive"
  ],
  "author": "Nathan Teetor <nathanteetor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nteetor/dull/issues"
  },
  "homepage": "https://github.com/nteetor/dull#readme"
}
