{
  "name": "yonder",
  "version": "0.0.5",
  "description": "A reactive framework built on shiny.",
  "main": "dist/js/yonder.min.js",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bootstrap": "^4.3.1",
    "bs-custom-file-input": "^1.3.1",
    "file-saver": "^1.3.8",
    "flatpickr": "^4.5.2",
    "ion-rangeslider": "^2.2.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "velocity-animate": "^1.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "babel-register": "^6.26.0",
    "clean-css": "^4.2.1",
    "clean-css-cli": "^4.2.1",
    "commander": "^2.19.0",
    "concat": "^1.0.3",
    "eslint": "^5.0.0",
    "glob": "^7.1.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^5.0.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-terser": "^1.0.1",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^2.5.0",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "test": "echo \"Sorry, nothing here\"",
    "js-lint": "eslint src/js/**/*.js",
    "js-vend": "cp dist/js/{yonder.js,yonder.min.js,yonder.min.js.map} ../inst/www/yonder/js/",
    "js-build": "rollup --config build/rollup.config.js --sourcemap",
    "js-minify": "uglifyjs --compress --mangle --output dist/js/yonder.min.js dist/js/yonder.js",
    "js": "npm-run-all js-lint js-build js-minify js-vend",
    "bootstrap-file-input-vend": "cp node_modules/bs-custom-file-input/dist/bs-custom-file-input.min.js ../inst/www/bootstrap/js/",
    "bootstrap-file-input": "npm-run-all bootstrap-file-input-vend",
    "bootstrap-js-vend": "cp node_modules/bootstrap/dist/js/bootstrap.min.js ../inst/www/bootstrap/js/",
    "bootstrap-js": "npm-run-all bootstrap-js-vend",
    "bootstrap-css-compile": "node-sass --quiet --output-style expanded src/scss/main.scss dist/css/bootstrap.css",
    "bootstrap-css-prefix": "postcss --config build/postcss.config.js --replace dist/css/bootstrap.css",
    "bootstrap-css-minify": "cleancss --level 1 --output dist/css/bootstrap.min.css dist/css/bootstrap.css",
    "bootstrap-css-vend": "cp dist/css/bootstrap.min.css ../inst/www/bootstrap/css/",
    "bootstrap-css": "npm-run-all bootstrap-css-compile bootstrap-css-prefix bootstrap-css-minify bootstrap-css-vend",
    "bootstrap": "npm-run-all bootstrap-js bootstrap-css bootstrap-file-input"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nteetor/yonder.git"
  },
  "keywords": [
    "shiny",
    "reactive"
  ],
  "author": "Nathan Teetor <nathanteetor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nteetor/yonder/issues"
  },
  "homepage": "https://github.com/nteetor/yonder#readme"
}
