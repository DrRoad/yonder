let fs = require('fs');

fs.copyFile('lib/css/dull.min.css', '../inst/www/dull/css/dull.min.css', (err) => {
  if (err) throw err;
  console.log('✔ copied dull.min.css');
});

fs.copyFile('lib/js/dull.min.js', '../inst/www/dull/js/dull.min.js', (err) => {
  if (err) throw err;
  console.log('✔ copied dull.min.js');
});
