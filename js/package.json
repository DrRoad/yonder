{
  "name": "yonder-js",
  "version": "0.1.2",
  "description": "A reactive framework built on shiny.",
  "main": "dist/js/yonder.min.js",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^10.0.2",
    "babel-preset-minify": "^0.5.0",
    "babel-register": "^6.26.0",
    "concat": "^1.0.3",
    "eslint": "^5.0.0",
    "glob": "^7.1.4",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "postcss-cli": "^5.0.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^1.0.1",
    "terser": "^4.3.8"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "vend": "cp dist/{yonder.js,yonder.min.js,yonder.min.js.map} ../inst/www/yonder/js/",
    "concat": "rollup --config build/rollup.config.js --sourcemap",
    "uglify": "uglifyjs --compress --mangle --output dist/yonder.min.js dist/yonder.js",
    "minify": "terser --config-file build/terser.config.json --output dist/yonder.min.js dist/yonder.js",
    "build": "run-s lint concat minify vend",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nteetor/yonder.git"
  },
  "keywords": [
    "shiny",
    "reactive"
  ],
  "author": "Nathan Teetor <nathanteetor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nteetor/yonder/issues"
  },
  "homepage": "https://nteetor.github.com/yonder",
  "dependencies": {}
}
