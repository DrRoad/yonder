{
  "name": "yonder-js",
  "version": "0.1.2",
  "description": "JavaScript for the yonder R package.",
  "main": "dist/js/yonder.min.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^10.0.2",
    "babel-preset-minify": "^0.5.0",
    "babel-register": "^6.26.0",
    "concat": "^1.0.3",
    "eslint": "^5.0.0",
    "glob": "^7.1.4",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^5.0.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^1.0.1",
    "terser": "^4.3.8"
  },
  "scripts": {
    "lint": "eslint --config build/eslint.config.json src/{index.js,*/*.js}",
    "vend": "cp dist/{yonder.js,yonder.min.js,yonder.min.js.map} ../inst/www/yonder/js/",
    "concat": "rollup --config build/rollup.config.js --sourcemap",
    "uglify": "uglifyjs --compress --mangle --output dist/yonder.min.js dist/yonder.js",
    "minify": "terser --config-file build/terser.config.json --output dist/yonder.min.js dist/yonder.js",
    "build": "run-s lint concat minify vend",
    "watch": "npm run build && nodemon --config build/nodemon.config.json --exec npm run build"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "browserslist": [
    ">= 1%",
    "last 1 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nteetor/yonder.git"
  },
  "keywords": [
    "shiny",
    "reactive",
    "yonder"
  ],
  "author": "Nathan Teetor <nathanteetor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nteetor/yonder/issues"
  },
  "homepage": "https://nteetor.github.com/yonder"
}
