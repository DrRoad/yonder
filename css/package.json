{
  "name": "yonder-css",
  "version": "0.1.2",
  "description": "CSS for the yonder R package.",
  "main": "dist/yonder.min.css",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "clean-css": "^4.2.1",
    "clean-css-cli": "^4.3.0",
    "concat": "^1.0.3",
    "eslint": "^5.0.0",
    "glob": "^7.1.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^5.0.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^2.5.0"
  },
  "scripts": {
    "compile": "node-sass --quiet --output-style expanded src/main.scss dist/yonder.css",
    "prefix": "postcss --config build/postcss.config.js --replace dist/yonder.css",
    "minify": "cleancss --level 1 --output dist/yonder.min.css dist/yonder.css",
    "vend": "cp dist/yonder.min.css ../inst/www/yonder/css/",
    "build": "run-s compile prefix minify vend"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nteetor/yonder.git"
  },
  "keywords": [
    "shiny",
    "reactive",
    "yonder"
  ],
  "author": "Nathan Teetor <nathanteetor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nteetor/yonder/issues"
  },
  "homepage": "https://nteetor.github.com/yonder"
}
